/dts-v1/;

/include/ "rt3883.dtsi"

/ {
	compatible = "F9K1103", "ralink,rt3883-soc";
	model = "Belkin F9K1103";

	palmbus@10000000 {
		spi@b00 {
			status = "okay";
			m25p80@0 {
				compatible = "mx25l6405d";
				reg = <0 0>;
				linux,modalias = "m25p80", "mx25l6405d";
				spi-max-frequency = <10000000>;

				partition@0 {
					label = "u-boot";
					reg = <0x0 0x30000>;
					read-only;
				};

				partition@30000 {
					label = "u-boot-env";
					reg = <0x30000 0x10000>;
					read-only;
				};

				factory: partition@40000 {
					label = "factory";
					reg = <0x40000 0x10000>;
					read-only;
				};

				partition@50000 {
					label = "firmware";
					reg = <0x50000 0x7b0000>;
				};
			};
		};

		gpio1: gpio@638 {
			status = "okay";
		};

		uartlite@c00 {
			status = "okay";
		};

		timer@100 {
			status = "okay";
		};
	};

	pinctrl {
		state_default: pinctrl0 {
			gpio {
				ralink,group = "i2c", "jtag", "uartf";
				ralink,function = "gpio";
			};
			mdio {
				ralink,group = "mdio";
				ralink,function = "mdio";
			};
		};
	};
	ethernet@10100000 {
			status = "okay";
       		mtd-mac-address = <&factory 0x4>;

			port@0 {
				ralink,fixed-link = <1000 1 1 1>;
				phy-mode = "rgmii";
			};
	};

	pci@10140000 {
		status = "okay";

		pinctrl-names = "default";
		pinctrl-0 = <&pci_pins>;

		pci_pins: pci {
			pci {
				ralink,group = "pci";
				ralink,function = "pci-fnc";
			};
		};

		host-bridge {
			pci-bridge@1 {
				status = "okay";
				};
			};
		};
	};

	wmac@10180000 {
		status = "okay";
		ralink,mtd-eeprom = <&factory 0x4>;
	};

	pci1 {
		status = "okay";
	};

	ehci@101c0000 {
		status = "okay";
	};

	ohci@101c1000 {
		status = "okay";
	};

	rtl8367b {
		compatible = "realtek,rtl8367b";
		gpio-sda = <&gpio0 1 1>;
		gpio-sck = <&gpio0 2 1>;
		realtek,extif1 = <1 0 1 1 1 1 1 1 2>;
		};

	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <100>;
		reset {
			label = "reset";
			gpios = <&gpio0 25 1>;
			linux,code = <0x198>;
		};
		wps {
			label = "wps";
			gpios = <&gpio0 26 1>;
			linux,code = <0x211>;
		};
	};

	gpio-leds {
		small-amber {
			label = "belkin:amber:small";
			gpios = <&gpio0 0 1>;
		};
		small-blue {
			label = "belkin:blue:small";
			gpios = <&gpio0 10 1>;
		};
		big-amber {
			label = "belkin:amber:big";
			gpios = <&gpio0 13 1>;
		};
		big-blue {
			label = "belkin:blue:big";
			gpios = <&gpio0 12 1>;
		};
		usb1 {
			label = "belkin:green:usb1";
			gpios = <&gpio0 9 1>;
		};

		usb2 {
			label = "belkin:green:usb2";
			gpios = <&gpio0 14 1>;
		};
	};
};
